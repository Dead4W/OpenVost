{
   "name": "Open AnimeVostorg",
   "short_name": "openvost",
   "manifest_version": 2,
   "author": "Ilya Zedgenizov",
   "content_scripts": [
   {
      "css": [ "css/main.css" ],
	  "js": [ "js/main.js" ],
	  "run_at": "document_end",
      "matches": [ "*://animevost.org/*" ]
   },
   {
   	  "js": [ "js/savemoment.js" ],
	  "run_at": "document_start",
      "matches": [ "*://aniland.org/*", "*://*.aniland.org/*" ]
   }
   ],
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "description": "Открывает дополнительные функций на сайте AnimeVost.org",
   "homepage_url": "https://vk.com/id167819261",
   "icons": {
      "32": "lib/openvost.png",
      "64": "lib/openvost-64x64.png",
      "128": "lib/openvost-128x128.png"
   },
   "key": "",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "permissions": [ "storage", "notifications", "*://animevost.org/*", "*://api.animevost.org/*" ],
   "web_accessible_resources" : ["js/inject.js", "img/cne.png", "img/cne_good.png", "lib/sound_push.wav", "js/openAnime.js"],
   "version": "1.0.9"
 }